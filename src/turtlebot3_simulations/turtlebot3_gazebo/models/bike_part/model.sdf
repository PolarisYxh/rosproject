      </inertial>

      <collision name="bike">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://bike_part/meshes/bike3.stl</uri>
          </mesh>
        </geometry>
      </collision>

      <visual name="visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://bike_part/meshes/bike3.stl</uri>
          </mesh>
        </geometry>
      </visual>  

  
	     <sensor name='bike_contact' type='contact'>
	       <contact>
		    <collision>bike</collision>    
	        </contact>
	       <update_rate> 5 </update_rate>
	    </sensor>
    </link>
